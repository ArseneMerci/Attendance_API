  "dependencies": {
    "@babel/cli": "^7.17.0",
    "@babel/core": "^7.17.0",
    "@babel/polyfill": "^7.12.1",
    "@babel/register": "^7.17.0",
    "@babel/runtime": "^7.17.0",
    "@sendgrid/mail": "^7.6.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "exceljs": "^4.3.0",
